{
  "name": "shodan-client",
  "version": "3.1.5",
  "description": "ðŸ‘€ A Node.js/JavaScript library for accessing the Shodan API",
  "keywords": [
    "node",
    "Shodan",
    "API",
    "client",
    "vulnerability",
    "0day",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/jesusprubio/shodan-client/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Jesus Rubio",
    "url": "http://jesusprubio.dev"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "contributors": [
    "https://github.com/jesusprubio/shodan-client/graphs/contributors"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jesusprubio/shodan-client/shodan-client.git"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "audit-ci": "^2.4.2",
    "depcheck": "^0.8.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "system-sleep": "^1.3.6"
  },
  "scripts": {
    "format": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "deps": "depcheck --ignores=eslint-config-airbnb-base,eslint-config-prettier,eslint-plugin-import,eslint-plugin-json,lint-staged,np,prettier",
    "audit": "audit-ci --moderate",
    "pretest": "npm run audit && npm run deps && npm run lint",
    "test": "nyc mocha --recursive --timeout 8000 test"
  },
  "engines": {
    "node": ">=10"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "strict": [
        "off"
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
